<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityC">

<TextView
    android:id="@+id/textView"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="177dp"
    android:layout_marginTop="129dp"
    android:layout_marginEnd="179dp"
    android:layout_marginBottom="53dp"
    android:text="Activity C"
    app:layout_constraintBottom_toTopOf="@+id/onFinishActivityC"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

<Button
    android:id="@+id/onFinishActivityC"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="118dp"
    android:layout_marginEnd="119dp"
    android:layout_marginBottom="482dp"
    android:onClick="onFinishActivityC"
    android:text="Finish Activity C"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>